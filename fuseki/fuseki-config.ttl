@prefix :        <#> .
@prefix fuseki:  <http://jena.apache.org/fuseki#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ja:      <http://jena.hpl.hp.com/2005/11/Assembler#> .
@prefix tdb:     <http://jena.hpl.hp.com/2008/tdb#> .
@prefix afn:     <http://jena.apache.org/ARQ/function#> .
@prefix spase:   <http://www.spase-group.org/data/schema/> .

[] rdf:type fuseki:Server ;
   fuseki:services (
     <#spase>
   ) .

<#spase> rdf:type fuseki:Service ;
    fuseki:name                       "spase" ;
    fuseki:serviceQuery               "query" ;
    fuseki:serviceQuery               "sparql" ;
    fuseki:serviceUpdate              "update" ;
    fuseki:serviceUpload              "upload" ;
    fuseki:serviceReadWriteGraphStore "data" ;
    fuseki:dataset                    <#dataset> .

<#dataset> rdf:type ja:RDFDataset ;
    ja:defaultGraph <#model> .

<#model> rdf:type      ja:MemoryModel ;
    ja:content [ja:externalContent <file:/fuseki-base/spase.ttl>] ;
    ja:content [ja:externalContent <file:/fuseki-base/spase.owl>] .
